# Shopify App Configuration
SHOPIFY_API_KEY=your_shopify_api_key_here
SHOPIFY_API_SECRET=your_shopify_api_secret_here
SCOPES=write_products,read_products,write_customers,read_customers
SHOPIFY_APP_URL=https://your-app-url.com

# Database
DATABASE_URL="file:./dev.db"

# AI Try-On Service
# ShootX ComfyUI Workflow on RunPod Configuration
RUNPOD_API_KEY=your_runpod_api_key_here
RUNPOD_ENDPOINT=https://api.runpod.ai/v2/your-endpoint-id
SHOOTX_COMFYUI_WORKFLOW_ID=shootx-tryon-workflow-v2
SHOOTX_BRANDING_ENABLED=true

# Image Processing Settings
MAX_IMAGE_SIZE=5242880
SUPPORTED_IMAGE_FORMATS=jpeg,jpg,png,webp

# ShootX Configuration
SHOOTX_VERSION=2.0
SHOOTX_QUALITY_PRESET=high
